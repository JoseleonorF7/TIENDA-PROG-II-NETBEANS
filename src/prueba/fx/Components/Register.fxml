<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prueba.fx.Controllers.LoginController">
    <TextField fx:id="idField" promptText="Número de Cédula"/>
    <TextField fx:id="nameField" promptText="Nombre"/>
    <TextField fx:id="phoneField" promptText="Teléfono"/>
    <TextField fx:id="emailField" promptText="Correo"/>
    <PasswordField fx:id="passwordField" promptText="Contraseña"/>
    <Button text="Registrarse" onAction="#handleRegister"/>
</VBox>
